{
    "presets": ["es2015", "react", "stage-0"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "utilities": "./utilities",
                    "animations": "./animations",
                    "groups": "./groups"
                }
            }
        ],
        "transform-react-remove-prop-types"
    ]
}
